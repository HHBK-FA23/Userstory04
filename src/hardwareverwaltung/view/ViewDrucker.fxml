<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" prefWidth="620.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hardwareverwaltung.controller.ControllerDrucker">
   <children>
      <Label text="Druckerverwaltung">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Label>
      <GridPane hgap="5.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="28.0" prefWidth="175.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="258.0" minWidth="158.0" prefWidth="173.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
        </rowConstraints>
         <children>
             <Label fx:id="labelId" text="Id" />
             <TextField fx:id="textfieldId" editable="false" GridPane.columnIndex="1" />
      
             <Label fx:id="labelSeriennummer" text="Seriennnummer" GridPane.rowIndex="1" />
             <TextField fx:id="textfieldSeriennummer" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      
             <Label fx:id="labelModell" text="Modell" GridPane.rowIndex="2" />
             <TextField fx:id="textfieldModell" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      
             <Label fx:id="labelHersteller" text="Hersteller" GridPane.rowIndex="3" />
             <TextField fx:id="textfieldHersteller" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      
             <Label fx:id="labelStatus" text="Status" GridPane.rowIndex="4" />
             <ChoiceBox fx:id="choiceboxStatus" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      
             <Label fx:id="labelHerstellergarantie" text="Herstellergarantie (Monate)" GridPane.rowIndex="5" />
             <TextField fx:id="textfieldHerstellergarantie" promptText="Angabe in Monaten" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      
             <Label fx:id="labelLieferdatum" text="Lieferdatum" GridPane.rowIndex="6" />
             <DatePicker fx:id="datepickerLieferdatum" GridPane.columnIndex="1" GridPane.rowIndex="6" />

             <Label fx:id="labelRaum" text="Raum" GridPane.rowIndex="7" />
             <ChoiceBox fx:id="choiceboxRaum" GridPane.columnIndex="1" GridPane.rowIndex="7" />

             <Label fx:id="labelFarbdruckfunktion" text="Farbdruckfunktion" GridPane.rowIndex="8" />
             <CheckBox fx:id="checkboxFarbdruckfunktion" GridPane.columnIndex="1" GridPane.rowIndex="8" />
      
             <Label fx:id="labelTechnologie" text="Technologie" GridPane.rowIndex="9" />
             <ChoiceBox fx:id="choiceboxTechnologie" GridPane.columnIndex="1" GridPane.rowIndex="9" />
      
             <Label fx:id="labelMaxPapierformat" text="max. Papierformat" GridPane.rowIndex="10" />
             <ChoiceBox fx:id="choiceboxMaxPapierformat" GridPane.columnIndex="1" GridPane.rowIndex="10" />
      
             <Label fx:id="labelAnzahlGedruckt" text="insgesamt gedruckt (Seitenanzahl)" GridPane.rowIndex="11" />
             <TextField fx:id="textfieldAnzahlGedruckt" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="11" />
      
             <Label fx:id="labelRestkapazitaetBetriebsmittel" text="RestkapazitÃ¤t Betriebsmittel (Seitenanzahl)" GridPane.rowIndex="12" />
             <TextField fx:id="textfieldRestkapazitaetBetriebsmittel" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="12" />
      
             <Label fx:id="labelMaxKapazitaet" text="Kapazitaet des Betriebsmittels (Seitenanzahl)" GridPane.rowIndex="13" />
             <TextField fx:id="textfieldMaxKapazitaet" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="13" />
      
      
         </children>
      </GridPane>
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
             <Button fx:id="buttonSichern" mnemonicParsing="false" onAction="#handleButtonSichern" text="Sichern" />
             <Button fx:id="buttonAbbrechen" mnemonicParsing="false" onAction="#handleButtonAbbrechen" text="Abbrechen" />
             <Button fx:id="buttonDashboard" mnemonicParsing="false" onAction="#handleButtonDashboard" text="Dashboard" />
         </children>
      </HBox>
      <ListView fx:id="listview" onMouseClicked="#handleMouseSelection" prefHeight="200.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
